{
    "name":"ProductiveApp",
    "description":"This app blocks all un-useful websites, on your command",
    "manifest_version": 3,
    "version": "1.0.0",
    "action":{
        "default_popup":"./popup.html"
    },
    "content_scripts": [
        {"matches": ["<all_urls>"],
        "css": ["contentScript.css"],
        "js": ["contentScript.js"]}
    ],
    "background":{
        "service_worker":"background.js"
    },
    "permissions": ["tabs", "activeTab", "storage"],

    "hosting_permissions":["<all_urls>"]
}